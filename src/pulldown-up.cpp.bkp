// RESISTOR PULL DOWN, RESISTOR PULL UP
#include <Arduino.h>
#define led 4
#define btn 3

// PULL DOWN PULL UP
// pull down -> o resistor do bot√£o fica no negativo, do pull down no positivo
void setup()
{
  pinMode(led, OUTPUT);
  pinMode(btn, INPUT);
}

void loop()
{
  if (digitalRead(btn) == HIGH)
  {
    digitalWrite(led, HIGH);
  }
  else
  {
    digitalWrite(led, LOW);
  }
}
